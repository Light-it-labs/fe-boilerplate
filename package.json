{
  "name": "fe-boilerplate",
  "private": true,
  "engines": {
    "node": ">=v20.9.0"
  },
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "pnpm with-env vite build",
    "clean": "git clean -xdf dist node_modules",
    "dev": "pnpm with-env vite",
    "preview": "pnpm with-env vite preview",
    "email": "email dev",
    "email:export": "email export --outDir './emails/exported-emails'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint .",
    "domain": "plop domain",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path ./.gitignore",
    "format:quick": "pretty-quick --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path ./.gitignore",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ./.env --",
    "build-staging": "pnpm with-env-staging vite build",
    "build-production": "pnpm with-env-production vite build",
    "with-env-staging": "dotenv -e ./.env.staging --",
    "with-env-production": "dotenv -e ./.env.production --",
    "prepare": "husky || true",
    "knip": "knip"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@sentry/integrations": "^7.114.0",
    "@sentry/react": "^8.47.0",
    "@sentry/tracing": "^7.114.0",
    "@sentry/vite-plugin": "^2.22.7",
    "@tanstack/react-query": "^5.62.8",
    "@tanstack/react-query-devtools": "^5.62.8",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "mem": "^10.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.1",
    "react-router-dom": "^7.0.2",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.3",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@react-email/components": "0.0.31",
    "@sentry/types": "^8.47.0",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/manager-api": "^8.4.7",
    "@storybook/preview-api": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/theming": "^8.4.7",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.12",
    "@types/uuid": "^10.0.0",
    "@vercel/style-guide": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "knip": "^5.40.0",
    "lint-staged": "^15.2.11",
    "plop": "^4.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "pretty-quick": "^4.0.0",
    "react-email": "3.0.4",
    "storybook": "^8.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.3"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {
        "config": "./tailwind.config.ts"
      },
      "autoprefixer": {}
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "pretty-quick --staged"
    ]
  },
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}
